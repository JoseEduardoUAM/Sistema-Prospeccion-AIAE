<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>Prospecto</title>
    <link rel="canonical" href="https://getbootstrap.com/docs/5.1/examples/dashboard/">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <style>
      .bd-placeholder-img {
        font-size: 1.125rem;
        text-anchor: middle;
        -webkit-user-select: none;
        -moz-user-select: none;
        user-select: none;
      }

      @media (min-width: 768px) {
        .bd-placeholder-img-lg {
          font-size: 3.5rem;
        }
      }
    </style>
    <link href="/Principal/dashboard.css" rel="stylesheet">
  </head>
  <body>
    
<header class="navbar navbar-dark sticky-top bg-dark flex-md-nowrap p-0 shadow">
  <a class="navbar-brand col-md-3 col-lg-2 me-0 px-3" href="/">AIAE</a>
  <button class="navbar-toggler position-absolute d-md-none collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#sidebarMenu" aria-controls="sidebarMenu" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="navbar-nav w-100"></div>
  <div class="navbar-nav">
    <div class="nav-item text-nowrap">
      <a class="nav-link px-3" href="/cerraSesion">Cerrar Sesion</a>
    </div>
  </div>
</header>

<div class="container-fluid">
  <div class="row">
    <nav id="sidebarMenu" class="col-md-3 col-lg-2 d-md-block bg-light sidebar collapse">
      <div class="position-sticky pt-3">
        <ul class="nav flex-column">
          <li class="nav-item">
            <a class="nav-link" aria-current="page" href="/">
              <span data-feather="home"></span>
              Inicio
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/tabla">
              <span data-feather="file"></span>
              Tabla
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/registro">
              <span data-feather="users"></span>
              Registrar
            </a>
          </li>
        </ul>
      </div>
    </nav>

    <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">

      <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
        <h1 class="h2">Prospecto</h1>
      </div>

      <div class="container px-4 py-4">
        <div class="row gx-5">
            <div class="col">
                <div class="p-3 border">
                    <h1 class="card-title mb-4">Contacto</h1>
                    <h5 class="card-title">Nombre:</h5>
                    <div id="Nombre" class="mb-3">--------------</div>
                    <h5 class="card-title">Telefono:</h5>
                    <div id="Telefono" class="mb-3">--------------</div>
                    <h5 class="card-title">Email:</h5>
                    <div id="Email" class="mb-3">--------------</div>
                    <h5 class="card-title">Cargo:</h5>
                    <div id="Cargo" class="mb-3">--------------</div>
                </div>
            </div>
            <div class="col">
                <div class="row gx-5">
                    <div class="col border p-3">
                        <h4 class="card-title mb-4">Observaciones</h4>
                        <div class="rounded p-2" style="background-color: rgb(28, 146, 101); color: aliceblue;" id="Observaciones">
                            --------------------
                        </div>
                    </div>
                </div>
                <div class="row gx-5 py-4">
                    <div class="col border p-3">
                        <h4 class="card-title mb-4">Retroalimentacion</h4>
                        <div id="Retroalimentaciones">
                            <!-- Aqui se colocan las retroalimentaciones -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="liveAlertPlaceholder"></div>
        <div class="row py-4" id="seccion_botones">
            <div class="row gx-3 border p-3">
                <div class="col-md-4 p-1">
                    <button type="button" class="btn btn-primary btn-lg" id="btn_modificar">Modificar</button>
                </div>
                <div class="col-md-4 p-1">
                    <button type="button" class="btn btn-danger btn-lg" id="btn_eliminar">Eliminar</button>
                </div>
            </div>
        </div>
        <div class="row py-4" id="seccion_modificacion" style="display: none;">
            <div class="row gx-3 border p-3">
                <h3 class="card-title">Modificar</h3>
                <form class="row g-3" id="Formulario">
                    <div class="col-md-12">
                      <label for="NombreM" class="form-label">Escribe el Nombre del Prospecto</label>
                      <input type="text" class="form-control" id="NombreM">
                    </div>
                    <div class="col-md-4">
                      <label for="TelefonoM" class="form-label">Escribe el Numero Telefonico</label>
                      <input type="text" class="form-control" id="TelefonoM">
                    </div>
                    <div class="col-md-4">
                      <label for="EmailM" class="form-label">Escribe el Correo Electronico</label>
                      <input type="email" class="form-control" id="EmailM">
                    </div>
                    <div class="col-md-4">
                      <label for="CargoM" class="form-label">Escribe el Cargo</label>
                      <input type="text" class="form-control" id="CargoM">
                    </div>
                    <div class="col-md-12">
                      <label for="ObservacionesM" class="form-label">Escribe las observaciones</label>
                      <!-- <input type="text" class="form-control" id="Observaciones"> -->
                      <textarea class="form-control" id="ObservacionesM"></textarea>
                    </div>
                    <div class="input-group">
                      <select class="form-select" id="OpcionesRetroalimentacion_Agregar">
                        <option selected>Selecciona las Retroalimentaciones</option>
                        <option value="1">Se envio correo electronico</option>
                        <option value="2">Opcion 2</option>
                        <option value="3">Opcion 3</option>
                        <option value="4">Opcion 4</option>
                        <option value="5">No se respondio el correo electronico</option>
                      </select>
                      <button class="btn btn-outline-secondary" type="button" id="btn_agregar_retro">Agregar</button>
                    </div>
                    <div class="input-group">
                        <select class="form-select" id="OpcionesRetroalimentacion_Quitar">
                          <option selected>Selecciona las Retroalimentaciones</option>
                          <option value="1">Se envio correo electronico</option>
                          <option value="2">Opcion 2</option>
                          <option value="3">Opcion 3</option>
                          <option value="4">Opcion 4</option>
                          <option value="5">No se respondio el correo electronico</option>
                        </select>
                        <button class="btn btn-outline-secondary" type="button" id="btn_quitar_retro">Quitar</button>
                      </div>
                    <ul class="list-group" id="ListaRetroalimentacion">
                    </ul>
                    <div id="liveAlertPlaceholder_Formulario"></div>
                    <div class="row">
                        <div class="col-md-4 p-4">
                            <button type="button" class="btn btn-success p-2" id="btn_enviar">Enviar</button>
                        </div>
                        <div class="col-md-4 p-4">
                            <button type="button" class="btn btn-danger p-2" id="btn_cancelar">Cancelar</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>


    </main>
  </div>
</div>

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/feather-icons@4.28.0/dist/feather.min.js" integrity="sha384-uO3SXW5IuS1ZpFPKugNNWqTZRRglnUJK6UAZ/gxOX80nxEkN9NcGZTftn6RzhGWE" crossorigin="anonymous"></script>

<script src="/Prospecto/Prospecto.js"></script>

</body>

</html>
